<UserControl x:Class="Wpf.BLPmodel.Pages.MenuPage.Views.MenuView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Wpf.BLPmodel.Pages.MenuPage.Views"
             mc:Ignorable="d" 
             d:DesignHeight="768" d:DesignWidth="1024">
    <Grid>

        <TextBlock Text="{Binding TikTak}" FontSize="90"></TextBlock>
        
        <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Height="100">
            <Button Command="{Binding GoToCommand}"
                CommandParameter="2"
                Content="Открыть первую стрицу"
                Margin="10" />
            <Button Command="{Binding GoToCommand}"
                CommandParameter="3"
                Content="Открыть вторую стрицу"
                Margin="10" />
            <Button Command="{Binding GoToCommand}"
                CommandParameter="4"
                Content="Открыть третью стрицу"
                Margin="10" />
        </StackPanel>      
        
    </Grid>   
  
</UserControl>
